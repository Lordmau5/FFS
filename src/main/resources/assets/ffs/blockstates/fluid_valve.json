{
  "multipart": [
    {
      "apply": {
        "model": "ffs:block/fluid_valve/base"
      }
    },
    {
      "when": {
        "tile_valid": "false"
      },
      "apply": {
        "model": "ffs:block/fluid_valve/invalid"
      }
    },
    {
      "when": {
        "tile_valid": "true"
      },
      "apply": {
        "model": "ffs:block/fluid_valve/valid"
      }
    },
    {
      "when": {
        "tile_main": "true"
      },
      "apply": {
        "model": "ffs:block/fluid_valve/main"
      }
    }
  ]
}
