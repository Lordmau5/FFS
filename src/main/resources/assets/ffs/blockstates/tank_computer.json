{
  "multipart": [
    {
      "when": {
        "tile_valid": "true"
      },
      "apply": {
        "model": "ffs:block/tank_computer/valid"
      }
    },
    {
      "when": {
        "tile_valid": "false"
      },
      "apply": {
        "model": "ffs:block/tank_computer/invalid"
      }
    }
  ]
}
